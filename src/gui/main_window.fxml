<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.Controller">
   <padding><Insets bottom="10" left="25" right="25" top="25" /></padding>
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
   <ComboBox prefWidth="150.0"
   GridPane.columnIndex="0" GridPane.rowIndex="1"/>
   <TextArea fx:id="commandInput" prefHeight="200.0" prefWidth="200.0"
   GridPane.columnIndex="0" GridPane.rowIndex="2"/>
   <Button fx:id="sendRequestButton" mnemonicParsing="false" onMouseClicked="#sendRequest" prefHeight="26.0" prefWidth="201.0" text="Send Request"
   GridPane.columnIndex="0" GridPane.rowIndex="3"/>
</GridPane>
